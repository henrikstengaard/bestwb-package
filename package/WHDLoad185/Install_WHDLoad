echo "Installing WHDLoad. Please wait a few minutes..."

Resident >NIL: unzip PURE
SetEnv TZ MST7

unzip -o WHDLoad185.zip -d SYS:

protect SYS:#? +rwe ALL

echo ""
echo ""
echo "Installation complete."
echo "Remove all floppies, reboot your Amiga, and enjoy!"
echo ""
echo "Goodbye"

wait 5 sec
quit